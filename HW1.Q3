#include <stdio.h>
#include <math.h>

int main(void)
{
	int popularity;
	int size;
	int homeValue;
	
	printf("Enter house popularity:\n");
	scanf("%d", &popularity);
	
	printf("Enter house size:\n");
	scanf("%d", &size);
	
	homeValue = (pow(popularity,3) + pow(size,2)) * 10000;
	printf("Home value is: %d\n", homeValue);
	
	return(0);
}
